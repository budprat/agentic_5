# LinkedIn Content Domination - Environment Configuration

# LinkedIn API Configuration
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret
LINKEDIN_ACCESS_TOKEN=your_linkedin_access_token
LINKEDIN_REDIRECT_URI=http://localhost:3000/callback

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4-turbo

# Fal.ai Configuration
FAL_AI_API_KEY=your_fal_ai_api_key

# ElevenLabs Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_VOICE_ID=your_preferred_voice_id

# Brightdata Configuration
BRIGHTDATA_USERNAME=your_brightdata_username
BRIGHTDATA_PASSWORD=your_brightdata_password
BRIGHTDATA_ENDPOINT=your_brightdata_endpoint

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=linkedin_domination
DB_USER=postgres
DB_PASSWORD=your_db_password

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379

# N8N Configuration
N8N_WEBHOOK_URL=your_n8n_webhook_url
N8N_API_KEY=your_n8n_api_key

# General Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Algorithm Weights (can be adjusted)
ALGORITHM_EARLY_ENGAGEMENT_WEIGHT=0.35
ALGORITHM_DWELL_TIME_WEIGHT=0.25
ALGORITHM_COMPLETION_RATE_WEIGHT=0.20
ALGORITHM_CREATOR_MODE_WEIGHT=0.10
ALGORITHM_CONSISTENCY_WEIGHT=0.10

# Content Configuration
CONTENT_GENERATION_BATCH_SIZE=10
CONTENT_QUALITY_THRESHOLD=7
BRAND_VOICE_CONSISTENCY_THRESHOLD=0.90

# Posting Configuration
OPTIMAL_POSTING_TIMES=07:30,12:00,17:00
POSTING_TIMEZONE=America/New_York
MAX_POSTS_PER_DAY=3

# Engagement Configuration
RESPONSE_TIME_TARGET=30
ENGAGEMENT_MONITOR_INTERVAL=60
VIRAL_THRESHOLD=100

# Analytics Configuration
ANALYTICS_RETENTION_DAYS=365
REPORT_FREQUENCY=daily