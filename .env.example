# Market Oracle Environment Variables
# Copy this file to .env and fill in your actual value

# Google APIs
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_PLACES_API_KEY=your_places_api_key_here_if_you_have_one

# Reddit API (Sentiment Seeker)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT="MarketOracle/1.0 by YourUsername"

# n8n (Trading automation)
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook
N8N_API_KEY=your_n8n_api_key

# Notion (Report documentation)
NOTION_API_KEY=your_notion_integration_token
NOTION_DATABASE_ID=your_market_research_database_id

# BrightData (Fundamental Analyst - SEC filings)
BRIGHTDATA_API_TOKEN=your_brightdata_api_token
BRIGHTDATA_CUSTOMER_ID=your_brightdata_customer_id
BRIGHTDATA_ZONE=mcp_unlocker

# Brave Search (News and web search)
BRAVE_API_KEY=your_brave_api_key

# HuggingFace (Technical Prophet - ML models)
HUGGINGFACE_API_KEY=your_huggingface_api_key
STOCK_MCP=https://tonic-stock-predictions.hf.space/gradio_api/mcp/sse

# Snowflake (Historical market data)
SNOWFLAKE_ACCOUNT=your_snowflake_account
SNOWFLAKE_USER=your_snowflake_username
SNOWFLAKE_PASSWORD=your_snowflake_password
SNOWFLAKE_DATABASE=SNOWFLAKE
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_ROLE=ACCOUNTADMIN
SNOWFLAKE_SCHEMA=CORTEX

# Supabase (Real-time portfolio tracking)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ElevenLabs (Audio briefings)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_VOICE_ID=your_voice_id

# Google Trends via SerpAPI
GOOGLE_TRENDS_API_KEY=your_serpapi_key

# Optional Market Data Providers
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key
POLYGON_API_KEY=your_polygon_key
IEX_CLOUD_API_KEY=your_iex_cloud_key

# Risk Management Settings
MAX_POSITION_SIZE=0.05
MAX_DAILY_LOSS=0.02
MAX_DRAWDOWN=0.15
HUMAN_OVERRIDE_THRESHOLD=10000

# Agent Configuration
ENABLE_PARALLEL_EXECUTION=true
AGENT_TIMEOUT_SECONDS=30
MAX_RETRIES=3

# Logging
LOG_LEVEL=INFO
LOG_FILE_PATH=logs/market_oracle.log

# Security
JWT_SECRET=generate_a_strong_secret_here
ENCRYPTION_KEY=generate_32_byte_key_here

# MCP Server Credentials (for .mcp.json)
SUPABASE_MCP_ACCESS_TOKEN=your_supabase_mcp_access_token
SUPABASE_MCP_PROJECT_REF=your_supabase_mcp_project_ref
FIRECRAWL_API_KEY=your_firecrawl_api_key
NOTION_MCP_TOKEN=your_notion_mcp_token
UPSTASH_EMAIL=your_upstash_email
UPSTASH_PROJECT_ID=your_upstash_project_id