{
  "name": "Sentiment Seeker BrightData",
  "description": "Reddit sentiment analysis using BrightData API and Stock Predictions MCP",
  "version": "2.0.0",
  "endpoint": "http://localhost:10502",
  "capabilities": [
    "reddit_sentiment_analysis",
    "brightdata_integration",
    "ml_stock_predictions",
    "volume_spike_detection"
  ],
  "specialization": "Social media sentiment using BrightData Reddit scraping and ML predictions",
  "model": {
    "provider": "google",
    "name": "gemini-2.0-flash",
    "temperature": 0.0
  },
  "external_apis": [
    {
      "name": "BrightData Reddit API",
      "endpoint": "https://api.brightdata.com/datasets/v3",
      "auth": "Bearer token",
      "dataset_id": "gd_lvz8ah06191smkebj4"
    },
    {
      "name": "Stock Predictions MCP",
      "endpoint": "https://tonic-stock-predictions.hf.space/gradio_api/mcp/sse",
      "type": "MCP SSE"
    }
  ],
  "data_sources": [
    "reddit_via_brightdata",
    "ml_predictions",
    "wallstreetbets",
    "stocks",
    "investing"
  ],
  "output_format": {
    "symbol": "TICKER",
    "sentiment_score": "-1.0 to 1.0",
    "confidence": "0.0 to 1.0",
    "volume_score": "low/normal/high/extreme",
    "ml_predictions": {
      "direction": "bullish/bearish",
      "confidence": "0.0 to 1.0",
      "predicted_move": "percentage"
    },
    "key_themes": ["theme1", "theme2"],
    "risk_flags": ["flag1", "flag2"],
    "recommendation": "bullish/neutral/bearish"
  },
  "integration": {
    "supabase_tables": [
      "sentiment_data",
      "trading_signals"
    ],
    "brightdata_config": {
      "keywords": ["stock symbol", "$symbol", "symbol stock"],
      "date_range": "All time",
      "sort_by": "Hot"
    }
  }
}