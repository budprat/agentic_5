{
  "name": "Trend Correlator",
  "description": "Google Trends analysis and search correlation agent",
  "version": "1.0.0",
  "endpoint": "http://localhost:10015",
  "capabilities": [
    "trend_analysis",
    "search_correlation",
    "predictive_signals",
    "lead_lag_analysis"
  ],
  "specialization": "Correlating search trends with market movements and identifying early signals",
  "model": {
    "provider": "google",
    "name": "gemini-2.0-flash",
    "temperature": 0.0
  },
  "tools": [
    {
      "name": "analyze_trends",
      "description": "Analyze Google Trends data for relevant search terms"
    },
    {
      "name": "calculate_correlation",
      "description": "Calculate correlation between search volume and price movements"
    },
    {
      "name": "identify_lead_lag",
      "description": "Determine if trends lead or lag price movements"
    }
  ],
  "data_sources": [
    "google_trends",
    "search_volumes",
    "product_launches",
    "brand_sentiment"
  ],
  "search_parameters": {
    "timeframe": "30_days",
    "geographic_scope": "worldwide",
    "category_filters": ["finance", "technology", "business"]
  },
  "output_format": {
    "trend_score": "0-100",
    "trend_direction": "increasing/decreasing/stable",
    "correlation_strength": "0.0-1.0",
    "lead_lag_analysis": {
      "trend_leads_price": "boolean",
      "lead_time_days": "number"
    },
    "key_search_terms": [
      {
        "term": "search term",
        "volume": "relative volume",
        "growth_rate": "percentage"
      }
    ]
  },
  "integration": {
    "supabase_tables": [
      "market_trends",
      "trading_signals"
    ],
    "mcp_connections": [
      "google_trends_mcp"
    ]
  }
}